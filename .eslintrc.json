{
  "extends": "airbnb",
  "installedESLint": true,
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "arrow-body-style": ["error", "as-needed"],
    "arrow-parens": 0,
    "comma-dangle": 0,
    "max-len": ["error", 80],
    "no-underscore-dangle": 0,
    "prefer-const": 0,
    "space-before-function-paren": 0
  }
}
